{
  "name": "@guasam/electron-react-app",
  "type": "module",
  "version": "0.4.1",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "description": "Internal library & components package for electron-react-app by Guasam",
  "author": "Guasam",
  "repository": {
    "type": "git",
    "url": "https://github.com/guasam/electron-react-app-lib"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./window": {
      "types": "./dist/window.d.ts",
      "import": "./dist/window.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "typesVersions": {
    "*": {
      "styles.css": [
        "./dist/styles.css"
      ],
      "window": [
        "./dist/window.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production bun run build.ts",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist",
    "dist/styles.css"
  ],
  "keywords": [
    "bun",
    "electron",
    "react",
    "library",
    "@guasam/electron-react-app",
    "electron-window",
    "custom-window-titlebar"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.1.10",
    "@types/react": "^19.0.7",
    "bun-plugin-dts": "^0.3.0",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "react": "^19.0.0"
  }
}
